# ============================================
# Pistology-WAIS v2.0 Configuration
# OpenAI-Powered Multi-Agent Warehouse System
# ============================================

# OpenAI API Configuration
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override default models for specific agents
# Defaults: All agents use gpt-4o
# SUPERVISOR_MODEL=gpt-4o
# INVENTORY_MODEL=gpt-4o
# OPERATIONS_MODEL=gpt-4o
# MATH_MODEL=gpt-4o

# System Configuration
DEBUG=False
LOG_LEVEL=INFO

# Agent Features
ENABLE_STREAMING=True
ENABLE_FUNCTION_CALLING=True
MAX_TOKENS=4096
MAX_RETRIES=3
TIMEOUT=30

# Document Processing
MAX_DOCUMENT_SIZE=10485760  # 10MB in bytes
CACHE_ENABLED=True
CACHE_TTL=3600  # 1 hour in seconds

# ============================================
# IMPORTANT NOTES:
# ============================================
# 1. This system now uses OpenAI SDK exclusively
# 2. Old Gemini and DeepSeek API keys are no longer needed
# 3. All agents use GPT-4o by default for best performance
# 4. Function calling is enabled for advanced tool use
# 5. Streaming provides real-time responses
# ============================================
